{
  "expo": {
    "name": "Word Check",
    "slug": "word-check",
    "privacy": "public",
    "scheme": "wordcheck",
    "platforms": ["ios", "android", "web"],
    "version": "51.0.0",
    "orientation": "default",
    "userInterfaceStyle": "automatic",
    "icon": "./assets/images/icon-1.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#151617"
    },
    "updates": {
      "url": "https://u.expo.dev/dd591e49-d2d1-4ce0-bef9-49746a819ec0"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "buildNumber": "1",
      "bundleIdentifier": "jonsamp.words",
      "supportsTablet": true
    },
    "android": {
      "versionCode": 6,
      "package": "com.jonsamp.wordcheck",
      "permissions": [],
      "icon": "./assets/images/icon-1.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-foreground.png",
        "backgroundImage": "./assets/images/adaptive-background.png"
      }
    },
    "web": {
      "bundler": "metro"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "extra": {
      "eas": {
        "projectId": "93429e94-accd-4d60-8a5c-0f292c313b74"
      }
    },
    "plugins": ["expo-font", "expo-asset", "expo-router"],
    "owner": "jonexpo"
  }
}
