{
  "cli": {
    "version": ">= 15.0.6",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "node": "22.12.0",
      "ios": {
        "resourceClass": "large"
      },
      "android": {
        "resourceClass": "large"
      }
    },
    "development-simulator": {
      "developmentClient": true,
      "distribution": "internal",
      "node": "22.12.0",
      "ios": {
        "simulator": true,
        "resourceClass": "large"
      },
      "android": {
        "buildType": "apk",
        "resourceClass": "large"
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "ios": {
        "resourceClass": "large"
      },
      "android": {
        "resourceClass": "large"
      }
    },
    "preview-simulator": {
      "ios": {
        "simulator": true,
        "resourceClass": "large"
      }
    },
    "production": {
      "autoIncrement": true,
      "channel": "production",
      "ios": {
        "resourceClass": "large"
      },
      "android": {
        "resourceClass": "large"
      }
    }
  },
  "submit": {
